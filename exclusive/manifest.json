{
    "manifest_version": 2,
    "name": "Exclusive",
    "version": "1.0",
    "description": "Protects the world from offensive news",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "128": "icons/exclusive.png"
    },
    "browser_action": {
        "default_icon": "icons/exclusive.png",
        "default_popup": "popup.html"
    },
    "permissions": [ "tabs", "http://localhost/" ]
}
